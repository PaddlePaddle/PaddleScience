FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
   

    left
    {
        type            fixedValue;
        value           uniform 7;
    }

    right
    {
        type            fixedValue;
        value           uniform 0;
    }

    
    frontAndBack
    {
        type            empty;
    }
}

